prime(n)
{
    i = 2;
    while(i * i < n + 1)
    {
        if(dels(n, i))
        {
            return 0;
        };
        i = i + 1;
    };

    return 1;
};

dels(x, y)
{
    if(x / y * y == x)
    {
        return 1;
    };

    return 0;
};

n = in;

i = 2;

answer = 0;

while(i < n)
{
    if(prime(i))
    {
        answer = answer + 1;
    };
    i = i + 1;
};

out(answer);